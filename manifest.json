{
  "manifest_version": 2,

  "name": "ChromeCtrlP",
  "description": "Help searching for tab/window just like the vim ctrlp does with files",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
